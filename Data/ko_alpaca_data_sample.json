[{"instruction": "\uac74\uac15\uc744 \uc720\uc9c0\ud558\uae30 \uc704\ud55c \uc138 \uac00\uc9c0 \ud301\uc744 \uc54c\ub824\uc8fc\uc138\uc694.", "input": "", "output": "\uc138 \uac00\uc9c0 \ud301\uc740 \uc544\uce68\uc2dd\uc0ac\ub97c \uaf2d \ucc59\uae30\uba70, \ucda9\ubd84\ud55c \uc218\uba74\uc744 \ucde8\ud558\uace0, \uc801\uadf9\uc801\uc73c\ub85c \uc6b4\ub3d9\uc744 \ud558\ub294 \uac83\uc785\ub2c8\ub2e4."}, {"instruction": "\uc138 \uac00\uc9c0 \uae30\ubcf8 \uc0c9\uc740 \ubb34\uc5c7\uc778\uac00\uc694?", "input": "", "output": "\uae30\ubcf8 \uc0c9\uc740 \ube68\uac15, \ud30c\ub791, \ub178\ub791\uc785\ub2c8\ub2e4."}, {"instruction": "\uc6d0\uc790\uc758 \uad6c\uc870\ub97c \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\uc6d0\uc790\ub294 \uc591\uc131\uc790, \uc911\uc131\uc790, \uc804\uc790\ub85c \uad6c\uc131\ub418\uc5b4 \uc788\uc73c\uba70, \uc591\uc131\uc790\uc640 \uc911\uc131\uc790\ub294 \uc6d0\uc790\ud575 \uc548\uc5d0 \uc788\uace0 \uc804\uc790\ub294 \uc8fc\ubcc0\uc5d0 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub300\uae30 \uc624\uc5fc\uc744 \uc5b4\ub5bb\uac8c \uc904\uc77c \uc218 \uc788\ub098\uc694?", "input": "", "output": "\ub300\uae30 \uc624\uc5fc\uc744 \uc904\uc774\uae30 \uc704\ud574 \uc790\ub3d9\ucc28 \ub300\uc2e0 \ub300\uc911\uad50\ud1b5\uc744 \uc774\uc6a9\ud558\uace0, \ubbf8\uc138\uba3c\uc9c0 \ubc30\ucd9c\ub7c9\uc744 \uc904\uc774\uae30 \uc704\ud574 \ucc28\ub7c9 \uc815\uae30 \uac80\uc0ac\ub97c \ubc1b\uc544\uc57c \ud569\ub2c8\ub2e4. \ub610\ud55c \uc5d0\ub108\uc9c0 \uc808\uc57d\uc744 \uc704\ud574 \uc804\uae30\ub97c \uc544\ub07c\uace0, \ub098\ubb34\ub97c \uc2ec\uc5b4 \ub300\uae30 \uc624\uc5fc\uc744 \uc904\uc77c \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc5b4\ub824\uc6b4 \uacb0\uc815\uc744 \ub0b4\ub824\uc57c \ud588\ub358 \ub54c\ub97c \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\uc81c\uac00 \uc5b4\ub824\uc6b4 \uacb0\uc815\uc744 \ub0b4\ub824\uc57c \ud588\ub358 \ub54c\ub294 \ub300\ud559\uc6d0 \uc878\uc5c5 \ud6c4 \uc9c1\uc7a5\uc744 \ucc3e\uc544\uc57c \ud588\ub358 \ub54c\uc785\ub2c8\ub2e4. \ub2e4\uc591\ud55c \uc120\ud0dd\uc9c0\uac00 \uc788\uc5c8\uc9c0\ub9cc \ub098\uc758 \uad00\uc2ec\uc0ac\uc640 \ube44\uc804\uc5d0 \uac00\uc7a5 \ubd80\ud569\ud558\ub294 \uacbd\ub825\uc744 \ubf51\ub294\ub370 \uc5b4\ub824\uc6c0\uc774 \uc788\uc5c8\uc2b5\ub2c8\ub2e4. \uc774 \ub54c \uc5ec\ub7ec \uc0ac\ub78c\ub4e4\uc758 \uc870\uc5b8\uacfc \ud604\uc7ac \uc0c1\ud669\uc5d0 \ub300\ud55c \uae4a\uc740 \uace0\ubbfc \ub05d\uc5d0 \uc801\uc808\ud55c \uc120\ud0dd\uc744 \ub0b4\ub9ac\uac8c \ub418\uc5c8\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ud640\uc218 \uc911 \ud558\ub098\ub97c \ubc1d\ud788\uc138\uc694.", "input": "\ud2b8\uc704\ud130, \uc778\uc2a4\ud0c0\uadf8\ub7a8, \ud154\ub808\uadf8\ub7a8", "output": "\ud154\ub808\uadf8\ub7a8\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubd84\uc218\uac00 1/4\uc5d0 \ud574\ub2f9\ud558\ub294 \uc774\uc720\ub97c \uc124\uba85\ud558\uc138\uc694.", "input": "4/16", "output": "\ubd84\ubaa8\uc640 \ubd84\uc790\ub97c \ubaa8\ub450 4\ub85c \ub098\ub20c \uc218 \uc788\uae30 \ub54c\ubb38\uc5d0 1/4\uc5d0 \ud574\ub2f9\ud569\ub2c8\ub2e4."}, {"instruction": "\uc911\uc694\ud55c \uc9c4\ub85c \uacb0\uc815\uc744 \ub0b4\ub824\uc57c \ud558\ub294 \uc8fc\uc778\uacf5\uc758 \uc774\uc57c\uae30\ub97c 3\uc778\uce6d \ub0b4\ub808\uc774\uc158\uc73c\ub85c \ub2e8\ud3b8 \uc18c\uc124\uc744 \uc4f0\uc138\uc694.", "input": "", "output": "\uc548\ub098\ub294 \uc5ec\ub7ec \ub300\ud559\uad50\uc758 \uc785\ud559 \uac00\ub2a5\uc131\uc774 \uc788\uc5c8\uc73c\ub098, \uc81c\ubc1c\uc2a4\ub7ec\uc6b4 \uc9c1\uc5c5\ub4e4\uc744 \ud5a5\ud574 \uc720\ud639 \ub2f9\ud558\ub294 \uacbd\ud5a5\uc774 \uc788\uc5b4 \ubcf8\uc778\uc774 \uac00\uc9c4 \uc9c4\ub85c\uc640 \uad00\uc2ec\uc0ac\uc5d0 \ub9de\ub3c4\ub85d \ucca0\uc800\ud788 \uac80\ud1a0\ud55c \uacb0\uacfc, \uac74\ucd95\ud559\uacfc\ub97c \uc900\ube44\ud558\ub294 \uacfc\uc815\uc5d0 \ub4e4\uc5b4\uac11\ub2c8\ub2e4. \ub531 \uc774 \uc2dc\uc810\uc5d0\uc11c, \uc548\ub098\ub294 \uacfc\uac70\uc758 \uc798\ubabb\ub41c \uc120\ud0dd \ub54c\ubb38\uc5d0 \ud655\uace0\ud55c \uacb0\uc815\uc744 \ub0b4\ub9ac\uae30 \ub9e4\uc6b0 \ud798\ub4e4\ub2e4\ub294 \uac83\uc5d0 \uace0\ud1b5\uc744 \uacaa\uac8c \ub429\ub2c8\ub2e4. \uacb0\uad6d, \uadf8\ub140\ub294 \uacfc\uac70\ub85c \ub3cc\uc544\uac00 \uc120\ud0dd\uc744 \uace0\uccd0\ubcf4\ub294 \uac83\uc740 \ubd88\uac00\ub2a5\ud558\ub2e4\ub294 \uac83\uc744 \uae68\ub2eb\uace0 \uc790\uc2e0\uc758 \ud310\ub2e8\uc5d0 \uadfc\uac70\ud558\uc5ec \ub354 \uae4a\uc774 \uace0\ubbfc\ud558\uba70 \uacc4\uc18d \ub098\uc544\uac00\uae30\ub85c \uacb0\uc815\ud569\ub2c8\ub2e4."}, {"instruction": "\uc9d1\uc758 3D \ubaa8\ub378\uc744 \ub80c\ub354\ub9c1\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc9d1\uc758 3D \ubaa8\ub378\ub9c1 \uacb0\uacfc\ubb3c \uc785\ub2c8\ub2e4. (\uc774\ubbf8\uc9c0 \ucca8\ubd80)"}, {"instruction": "\uc774 \ubb38\uc7a5\uc5d0 \ucca0\uc790\uc640 \ubb38\ubc95 \uc624\ub958\uac00 \uc788\ub294\uc9c0 \ud3c9\uac00\ud558\uc138\uc694.", "input": "\uadf8\ub294 \uc2dd\uc0ac\ub97c \ub9c8\uce58\uace0 \uc2dd\ub2f9\uc744 \ub098\uc130\uc2b5\ub2c8\ub2e4.", "output": "\uadf8 \ubb38\uc7a5\uc5d0\ub294 \ucca0\uc790\ub098 \ubb38\ubc95\uc5d0 \ub300\ud55c \uc624\ub958\uac00 \uc5c6\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc904\ub9ac\uc5b4\uc2a4 \uc2dc\uc800\ub294 \uc5b4\ub5bb\uac8c \uc8fd\uc5c8\ub098\uc694?", "input": "", "output": "\uc904\ub9ac\uc5b4\uc2a4 \uc2dc\uc800\ub294 \uadf8\uc758 \uce5c\uad6c\uc778 \ub9c8\ub974\ucfe0\uc2a4 \ube0c\ub8e8\ud22c\uc2a4\uc5d0\uac8c \uc0b4\ud574\ub2f9\ud588\ub2e4\uace0 \uc54c\ub824\uc838 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ud504\ub791\uc2a4\uc758 \uc218\ub3c4\ub294 \ubb34\uc5c7\uc778\uac00\uc694?", "input": "", "output": "\ud504\ub791\uc2a4\uc758 \uc218\ub3c4\ub294 \ud30c\ub9ac\uc785\ub2c8\ub2e4."}, {"instruction": "\ucea0\ud551 \uc5ec\ud589\uc5d0 \ud544\uc694\ud55c 10\uac00\uc9c0 \ubb3c\ud488\uc758 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud558\uc138\uc694.", "input": "", "output": "\ud150\ud2b8, \uc7a0\uc790\ub9ac, \ub79c\ud134, \uc544\uc774\uc2a4\ucfe8\ub7ec, \uc2dd\uc74c\ub8cc, \ud3ec\ud06c, \ub098\uc774\ud504, \ubd88\uc528, \uce58\uc57d, \ucea0\ud551\uc758\uc790"}, {"instruction": "\ub300\uacf5\ud669\uc758 \uc6d0\uc778\uc5d0 \ub300\ud574 \ud1a0\ub860\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\ub300\uacf5\ud669\uc740 \uae08\uc735\uc2dc\uc7a5\uacfc \ubbf8\uad6d\uc758 \uae30\uc5c5\uacfc \uacbd\uc81c \uc0b0\uc5c5\uc758 \uaddc\uc81c \ubc0f \uade0\ud615\uc744 \uae68\ub728\ub824 \ubd88\uc548\uc744 \uc870\uc131\ud558\uace0 \ubd88\uc548\uc774 \uc77c\uc5b4\ub098\uba74\uc11c \uc0dd\uae34 \uacbd\uc81c \uc704\uae30\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c\uc744 \ub3d9\ubb3c, \uc2dd\ubb3c, \uad11\ubb3c\ub85c \ubd84\ub958\ud558\uc2ed\uc2dc\uc624.", "input": "\ucc38\ub098\ubb34, \uad6c\ub9ac \uad11\uc11d, \ucf54\ub07c\ub9ac", "output": "\ub3d9\ubb3c: \ucf54\ub07c\ub9ac, \uc2dd\ubb3c: \ucc38\ub098\ubb34, \uad11\ubb3c: \uad6c\ub9ac \uad11\uc11d"}, {"instruction": "\uc790\uc5f0\uc5b4 \ucc98\ub9ac\uc5d0\uc11c \ub2e8\uc5b4 \uc784\ubca0\ub529\uc758 \uc0ac\uc6a9\uc744 \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\ub2e8\uc5b4 \uc784\ubca0\ub529\uc740 \ub2e8\uc5b4\ub97c \ubca1\ud130\ub85c \ub9e4\ud551\ud558\uc5ec \ub2e8\uc5b4 \uac04 \uad00\uacc4 \ubc0f \uc720\uc0ac\uc131\uc744 \uacc4\uc0b0\ud558\ub294 \uae30\uc220\uc785\ub2c8\ub2e4. \uc774 \uae30\uc220\uc740 \ubb38\uc11c \ubd84\ub958, \uac10\uc131 \ubd84\uc11d, \uae30\uacc4 \ubc88\uc5ed \ub4f1 \ub2e4\uc591\ud55c \ubd84\uc57c\uc5d0\uc11c \uc0ac\uc6a9\ub429\ub2c8\ub2e4."}, {"instruction": "\ucef4\ud4e8\ud130 \ub9c8\ub354\ubcf4\ub4dc\uc758 \uae30\ub2a5 \uc124\uba85\ud558\uae30", "input": "", "output": "\ub9c8\ub354\ubcf4\ub4dc\ub294 \ucef4\ud4e8\ud130\uc758 \ubaa8\ub4e0 \uad6c\uc131 \uc694\uc18c\ub97c \uc5f0\uacb0\ud558\uace0 \ud1b5\uc2e0\uc744 \ub2f4\ub2f9\ud558\ub294 \uc8fc\uc694 \ucef4\ud4e8\ud130 \ubd80\ud488\uc785\ub2c8\ub2e4. \uc774\ub294 CPU, RAM, \uadf8\ub798\ud53d \uce74\ub4dc, \uc624\ub514\uc624 \uce74\ub4dc, \ub124\ud2b8\uc6cc\ud06c \uce74\ub4dc \ubc0f \uae30\ud0c0 \uc7a5\uce58\uc640 \ud1b5\uc2e0\ud569\ub2c8\ub2e4."}, {"instruction": "\uc774 \ucf54\ub4dc\ub97c \ub9ac\ubc84\uc2a4 \uc5d4\uc9c0\ub2c8\uc5b4\ub9c1\ud558\uc5ec \uc0c8 \ubc84\uc804 \ub9cc\ub4e4\uae30", "input": "def factorialize(num):\n  \uacc4\uc2b9 = 1\n  for i in range(1, num):\n    \uacc4\uc2b9 *= i\n  \n  \ubc18\ud658 \uacc4\uc2b9", "output": "\uc785\ub825\ub41c \ucf54\ub4dc\uc5d0 \ub300\ud55c \ub9ac\ubc84\uc2a4 \uc5d4\uc9c0\ub2c8\uc5b4\ub9c1 \uacb0\uacfc\ub294 \uc544\ub798\uc640 \uac19\uc2b5\ub2c8\ub2e4:\n \n def factorialize(num):\n  result = 1\n  for i in range(2, num+1):\n    result *= i \n  return result"}, {"instruction": "\ub370\uc774\ud130 \ud504\ub77c\uc774\ubc84\uc2dc \ubb38\uc81c\uc5d0 \ub300\ud55c \uc724\ub9ac\uc801 \ud574\uacb0\ucc45 \uc81c\uc548\ud558\uae30", "input": "", "output": "\ub370\uc774\ud130 \ud504\ub77c\uc774\ubc84\uc2dc \ubb38\uc81c\ub97c \ud574\uacb0\ud558\uae30 \uc704\ud55c \uc724\ub9ac\uc801\uc778 \ub300\uc548\uc73c\ub85c\ub294 \uc0ac\uc6a9\uc790 \uac1c\uc778 \uc815\ubcf4\ub97c \uc218\uc9d1\ud558\ub294 \ud68c\uc0ac\uc5d0 \ub300\ud55c \uaddc\uc81c \ubc0f \uc81c\ud55c, \uac1c\uc778 \uc815\ubcf4\ubcf4\ud638 \uac15\ud654, \ub370\uc774\ud130 \uc8fc\uccb4\uc758 \uad8c\ub9ac \uaddc\uc815 \ud655\ub300 \ub4f1\uc774 \uc788\uc2b5\ub2c8\ub2e4. \ub610\ud55c \uc778\uacf5\uc9c0\ub2a5\uacfc \ube45\ub370\uc774\ud130 \ubd84\uc57c\uc5d0\uc11c\uc758 \uc724\ub9ac\uc801 \ubb38\uc81c\ub97c \ub17c\uc758\ud558\ub294 \uacf5\ub860\ud654\uc640 \uad50\uc721 \ud504\ub85c\uadf8\ub7a8\ub4f1\uc744 \ub9c8\ub828\ud558\uc5ec \uc804\ubb38\uac00\uc640 \uc77c\ubc18 \uc0ac\uc6a9\uc790\uac00 \ub370\uc774\ud130 \ud504\ub77c\uc774\ubc84\uc2dc \ubb38\uc81c\uc5d0 \ub300\ud574 \uc801\uadf9\uc801\uc73c\ub85c \uad00\uc2ec\uc744 \uac00\uc9c0\uace0 \ub300\uc751\ud560 \uc218 \uc788\ub3c4\ub85d \uc9c0\uc6d0\ud558\ub294 \uac83\uc774 \ud544\uc694\ud569\ub2c8\ub2e4."}, {"instruction": "\"\uc0ac\uacfc\ud558\ub2e4\"\uc640 \uac19\uc740 \ub73b\uc744 \uac00\uc9c4 \ub3d9\uc0ac \uc138 \uac1c \uc0dd\uc131\ud558\uae30", "input": "", "output": "\uc870\uc218\ud558\ub2e4, \ud574\uacb0\ud558\ub2e4, \ubaa8\uba74\ud558\ub2e4"}, {"instruction": "DNA\ub294 \ubb34\uc5c7\uc744 \uc758\ubbf8\ud560\uae4c\uc694?", "input": "", "output": "DNA\ub294 \ub514\uc625\uc2dc\ub9ac\ubcf4\ud575\uc0b0(Deoxyribonucleic acid)\uc758 \uc904\uc784\ub9d0\ub85c, \uc720\uae30 \ubd84\uc790\ub85c \uad6c\uc131\ub41c \uc720\uc804 \ubb3c\uc9c8\uc785\ub2c8\ub2e4."}, {"instruction": "\ucfe0\ubc14 \ubbf8\uc0ac\uc77c \uc704\uae30\uc640 \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc744 \ube44\uad50\ud558\uace0 \ub300\uc870\ud558\uc138\uc694.", "input": "", "output": "\ucfe0\ubc14 \ubbf8\uc0ac\uc77c \uc704\uae30\ub294 \ub0c9\uc804 \uc2dc\uae30\uc758 \uc804\ub7b5 \uacbd\uc7c1\uc758 \uc911\uc694\ud55c \uc0ac\uac74 \uc911 \ud558\ub098\ub85c, \ubbf8\uad6d\uacfc \uc18c\ub828 \uc0ac\uc774\uc758 \uad70\uc0ac\uc801 \uac08\ub4f1\uc744 \uac00\uc838\uc654\uc2b5\ub2c8\ub2e4. \ubc18\uba74\uc5d0 \ubca0\ud2b8\ub0a8 \uc804\uc7c1\uc740 1960\ub144\ub300\uc640 1970\ub144\ub300\uc5d0 \uc774\uc5b4\uc9c4 \ubbf8\uad6d\uc758 \uc785\uc7a5\uc5d0\uc11c \ucda9\ub3cc\uc774\uc5c8\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ubb34\uc791\uc704 \ub2e8\uc5b4 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc74c\uacc4, \uc11c\uc801, \uc0c9\uc0c1, \uad6d\uac00, \uacfc\uc77c \ub4f1 \uce74\ud14c\uace0\ub9ac\uc5d0 \ub9de\ucdb0 \ubb34\uc791\uc704\ub85c \ubaa9\ub85d\uc744 \uc0dd\uc131\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub179\uc74c \ub0b4\uc6a9\uc744 \ud14d\uc2a4\ud2b8\ub85c \ubcc0\ud658\ud569\ub2c8\ub2e4.", "input": "\uc6cc\ud138\ub8e8\uc758 \uc5ed\uc0ac\uc5d0 \ub300\ud55c \uae30\ub85d", "output": "\"\uc6cc\ud138\ub8e8\"\ub294 \uc720\ub839 \uac1c\uc120\uad6c\uc5ed\uc744 \uc81c\uc678\ud55c \ubbf8\uad6d \uc544\uc774\uc624\uc640 \uc8fc\uc758 \uc911\ubd80\uc5d0 \uc704\uce58\ud55c \ub3c4\uc2dc\uc785\ub2c8\ub2e4. \ubbf8\uad6d \ub0b4\uc5d0\uc11c\ub3c4 \uc720\uba85\ud558\uc9c0 \uc54a\uc740 \ub3c4\uc2dc\uc774\uace0, \uc6cc\ud138\ub8e8\uc758 \uc5ed\uc0ac\uc5d0 \ub300\ud55c \uae30\ub85d\uc744 \ucc3e\uae30 \uc704\ud574\uc11c\ub294 \uc9c0\uc5ed \ub3c4\uc11c\uad00\uacfc \uac19\uc740 \uacf3\uc744 \ubc29\ubb38\ud574\uc57c \ud560 \uac83\uc785\ub2c8\ub2e4."}, {"instruction": "\uc138\uacc4\uc5d0\uc11c \uac00\uc7a5 \uc720\uba85\ud55c \ud654\uac00\ub294 \ub204\uad6c\uc77c\uae4c\uc694?", "input": "", "output": "\uc138\uacc4\uc5d0\uc11c \uac00\uc7a5 \uc720\uba85\ud55c \ud654\uac00\ub294 \ub808\uc624\ub098\ub974\ub3c4 \ub2e4 \ube48\uce58(Laonardo da Vinci)\uc785\ub2c8\ub2e4."}, {"instruction": "\ucf54\uae45 \ud1a0\ud06c\uc758 \uac1c\ub150\uc744 \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\ucf54\uae45 \ud1a0\ud06c\ub294 \ud504\ub85c\uc81d\ud2b8 \ub9e4\ub2c8\uc800\uc640 \ud300\uc6d0 \uc0ac\uc774\uc758 \ub300\ud654\ub97c \uc9c4\ud589\ud558\ub294 \ubc29\uc2dd\uc73c\ub85c, \ub9ce\uc740 \ud504\ub85c\uc81d\ud2b8\uc5d0\uc11c \ub110\ub9ac \uc0ac\uc6a9\ub418\ub294 \ubbf8\ud305 \ubc29\ubc95\uc785\ub2c8\ub2e4."}, {"instruction": "\ubb3c\uc758 \ub053\ub294\uc810\uc744 \ucc3e\uc544\ubcf4\uc138\uc694.", "input": "", "output": "\ubb3c\uc758 \ub053\ub294\uc810\uc740 \uc57d 100\ub3c4\uc785\ub2c8\ub2e4. \uadf8\ub7ec\ub098 \ubb3c\uc758 \ub053\ub294\uc810\uc740 \ud574\ubc1c \uace0\ub3c4\uc640 \uac19\uc740 \uc678\ubd80 \uc694\uc778\uc5d0 \uc758\ud574 \ubcc0\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ucc30\uc2a4 2\uc138\uc758 \uc0dd\uc560\uc640 \ud1b5\uce58\uc5d0 \ub300\ud574 \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\ucc30\uc2a4 2\uc138\ub294 17\uc138\uae30 \uc720\ub7fd\uc5d0 \uc788\ub294 \uc5ec\ub7ec \uad6d\uac00 \uc911 \ud558\ub098\uc778 \uc601\uad6d\uc758 \uad6d\uc655\uc774\uc5c8\uc73c\uba70, \ub300\uad00\ub839 \uc804\ud22c\uc640 \uac19\uc740 \uc720\uba85\ud55c \uc5ed\uc0ac\uc801 \uc0ac\uac74\uc5d0\uc11c \uc911\uc694\ud55c \uc5ed\ud560\uc744 \ud588\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ubc18\uc9c0\ub984\uc774 \uc8fc\uc5b4\uc9c4 \uc6d0\uc758 \ub113\uc774\ub97c \uad6c\ud569\ub2c8\ub2e4.", "input": "\ubc18\uacbd = 4", "output": "\uc785\ub825\ub41c \ubc18\uc9c0\ub984 \uac12(4)\uc744 \uc774\uc6a9\ud558\uc5ec \uc6d0\uc758 \ub113\uc774\ub294 16pi\uc785\ub2c8\ub2e4."}, {"instruction": "\ubd81\ubbf8\uc640 \ub0a8\ubbf8\ub97c \ub098\ub204\ub294 \uacbd\ub3c4\ub97c \uad6c\ud569\ub2c8\ub2e4.", "input": "", "output": "\ubd81\ubbf8\uc640 \ub0a8\ubbf8\ub97c \ub098\ub204\ub294 \uacbd\ub3c4\ub294 60\u00b0 \uc11c\uacbd\uc785\ub2c8\ub2e4."}, {"instruction": "\uc2dd\ubb3c\uc758 \uc138\ud3ec \ud638\ud761 \uacfc\uc815\uc744 \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc2dd\ubb3c\uc740 \ud638\ud761 \uacfc\uc815 \uc911 \uc77c\uc0b0\ud654\ud0c4\uc18c\uc640 \ubb3c\uc5d0\uc11c \uc5d0\ub108\uc9c0\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4. \uc774 \uc591\ubd84\uc740 \ud6c4\uc5d0 ATP\uc640 \uac19\uc740 \uc138\ud3ec \uc5d0\ub108\uc9c0\ub85c \uc804\ud658\ub429\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \ub2a5\ub3d9\ud0dc\ub97c \uc0ac\uc6a9\ud558\uc5ec \ub2e4\uc2dc \uc4f0\uc138\uc694.", "input": "\ub274\uc2a4 \ubcf4\ub3c4\uac00 \uae30\uc7a5\uc5d0 \uc758\ud574 \uc77d\ud614\uc2b5\ub2c8\ub2e4.", "output": "\uae30\uc7a5\uc774 \ub274\uc2a4 \ubcf4\ub3c4\ub97c \uc77d\uc5c8\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc5b4\ub5a4 \uc0ac\ub78c\uc744 \uc6a9\uac10\ud558\ub2e4\uace0 \ubb18\uc0ac\ud558\ub294 \ud615\uc6a9\uc0ac \ubaa9\ub85d\uc744 \uc0dd\uc131\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc6a9\uae30 \uc788\uac8c, \ub300\ub2f4\ud558\uac8c, \uc6b0\uae30\ub294 \uac83\uc744 \ub450\ub824\uc6cc\ud558\uc9c0 \uc54a\uace0, \ubd88\uc548\uac10 \uc5c6\uc774, \ubd88\uad74\uc758 \uc815\uc2e0\uc73c\ub85c."}, {"instruction": "\uc0bc\ub9bc \ubc8c\ucc44\uc758 \uacb0\uacfc\ub97c \uac1c\ub7b5\uc801\uc73c\ub85c \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc0bc\ub9bc \ubc8c\ucc44\ub294 \uc0b0\ub9bc\uc0dd\ud0dc\uacc4\uc5d0 \ud070 \ubd80\uc791\uc6a9\uc744 \ucd08\ub798\ud569\ub2c8\ub2e4. \uc774\ub85c \uc778\ud574 \ud3ed\ubc1c\uc801\uc73c\ub85c \uc0dd\uae30\ub294 \ud1a0\uc591\uce68\uc2dd, \uc0dd\ud0dc\uc801 \ubd88\uade0\ud615\uacfc \uae30\ud6c4\ubcc0\ud654\uc758 \uc545\uc601\ud5a5\uc744 \ucd08\ub798\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uac00\uc815\uc5d0\uc11c \uc804\uae30 \uc0ac\uc6a9\ub7c9\uc744 \uc904\uc774\uae30 \uc704\ud55c \uacc4\ud68d\uc744 \uc138\uc6b0\uc138\uc694.", "input": "", "output": "\uad50\ud1b5\uc218\ub2e8 \ub300\uc2e0 \ub3c4\ubcf4\ub098 \uc790\uc804\uac70\ub97c \uc774\uc6a9\ud558\uace0, \uc804\uae30\uc81c\ud488\uc744 \uaebc\ub193\uace0 \uc18c\ub4f1\ud558\uba70, \uc5d0\ub108\uc9c0 \ud6a8\uc728\uc801\uc778 \uc218\uc555\uc870\ub97c \uc0ac\uc6a9\ud558\ub294 \ub4f1\uc758 \ubc29\ubc95\uc73c\ub85c \uc804\uae30 \uc0ac\uc6a9\ub7c9\uc744 \uc904\uc77c \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ubb38\uc7a5\uc758 \ub2e8\uc5b4\ub4e4\uc744 \ubc30\uc5f4\ud558\uc5ec \ubb38\ubc95\uc801\uc73c\ub85c \uc62c\ubc14\ub978 \ubb38\uc7a5\uc744 \uad6c\uc131\ud569\ub2c8\ub2e4.", "input": "\uac08\uc0c9 \uc5ec\uc6b0\uac00 \ube68\ub9ac \ub6f0\uc5c8\uc2b5\ub2c8\ub2e4.", "output": "\uac08\uc0c9 \uc5ec\uc6b0\uac00 \ube68\ub9ac \ub6f0\uc5c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ud14d\uc2a4\ud2b8\uc758 \uc5b4\uc870\ub97c \ubd84\uc11d\ud569\ub2c8\ub2e4.", "input": "\uc804 \uc138\uacc4\ub294 \ucf54\ub85c\ub09819 \ud32c\ub370\ubbf9\uc73c\ub85c \uc778\ud574 \ud070 \uc601\ud5a5\uc744 \ubc1b\uc558\uc73c\uba70, \uc774\ub294 \uc6b0\ub9ac\uc758 \uc0b6\uc744 \ud06c\uac8c \ubcc0\ud654\uc2dc\ucf30\uc2b5\ub2c8\ub2e4.", "output": "\uc785\ub825\ub41c \ubb38\uc7a5\uc740 \uae0d\uc815\uc801\uc778 \uc5b4\uc870\ub97c \uac00\uc9c0\uace0 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ub370\uc774\ud130\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc911\uc559\uac12\uc744 \uacc4\uc0b0\ud569\ub2c8\ub2e4.", "input": "[2, 3, 7, 8, 10]", "output": "\uc8fc\uc5b4\uc9c4 \ub370\uc774\ud130\uc758 \uc911\uc559\uac12\uc740 7\uc785\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ub2e8\ub77d\uc744 \ub354 \uc9e7\uace0 \uc774\ud574\ud558\uae30 \uc26c\uc6b4 \ud615\ud0dc\ub85c \ub2e4\uc2dc \uc791\uc131\ud569\ub2c8\ub2e4.", "input": "\uc778\ud130\ub137 \ub355\ubd84\uc5d0 \uc804 \uc138\uacc4 \uc0ac\ub78c\ub4e4\uacfc \uc5f0\uacb0\ub420 \uc218 \uc788\ub2e4\ub294 \uac83\uc740 \uc77c\ubc18\uc801\uc73c\ub85c \uc778\uc815\ub418\uc9c0\ub9cc, \uc778\ud130\ub137\uc758 \uae30\ubcf8 \uae30\ub2a5\uc5d0 \uc775\uc219\ud558\uc9c0 \uc54a\uac70\ub098 \uc778\ud130\ub137\uc774 \uc65c \uadf8\ub807\uac8c \ubcf4\ud3b8\ud654\ub418\uc5c8\ub294\uc9c0, \uc778\ud130\ub137\uc758 \uc9c4\uc815\ud55c \uae30\ub2a5\uc774 \ubb34\uc5c7\uc778\uc9c0 \uc774\ud574\ud558\uc9c0 \ubabb\ud558\ub294 \uc0ac\ub78c\ub4e4\uc774 \uc5ec\uc804\ud788 \uc874\uc7ac\ud569\ub2c8\ub2e4.", "output": "\uc778\ud130\ub137\uc758 \ud575\uc2ec \uc0ac\uc6a9 \ubc29\ubc95 \uac00\uc6b4\ub370 \uc77c\ubd80 \uae30\ub2a5\uc5d0 \ub300\ud55c \uc811\uadfc\uc131\uc774 \ub0ae\uc740 \uc0c1\ud669\uc5d0\uc11c \uc778\ud130\ub137\uc744 \ubcf4\ud3b8\ud654 \uc2dc\ud0a4\ub294 \uc791\uc5c5\uc774 \uacc4\uc18d \uc9c4\ud574\ud574 \uc654\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc7ac\ud65c\uc6a9\uc5d0 \uad00\ud55c \uc6f9\uc0ac\uc774\ud2b8\uc758 \ub85c\uace0\ub97c \ub514\uc790\uc778\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc785\ub825\uc774 \uc5c6\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uae30\uc068\uc744 \ud45c\ud604\ud558\ub294 \uc2dc\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\ub098\uc758 \ud558\uc580 \uba4d\uba4d\uc774, \ub108\uc758 \uaf2c\ub9ac\ub294 \uae38\uc9c0\ub9cc, \ub108\ub294 \ub098\ub97c \uc0ac\ub791\ud574\uc904\uc218\ub85d \ub354 \uc88b\uc544\uc838."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ubc29\uc815\uc2dd\uc744 \ub300\uc218\uc2dd\uc73c\ub85c \ubcc0\ud658\ud569\ub2c8\ub2e4.", "input": "3x+5y=9", "output": "3x + 5y = 9\ub97c \ub300\uc218\uc2dd\uc73c\ub85c \ubcc0\ud658\ud558\uba74 -5y + 3x = 9\uc785\ub2c8\ub2e4."}, {"instruction": "\uc218\uc9c8 \uc624\uc5fc\uc744 \uc904\uc774\uae30 \uc704\ud55c \uac00\ub2a5\ud55c \ud574\uacb0\ucc45\uc744 \ube0c\ub808\uc778\uc2a4\ud1a0\ubc0d\ud558\uc138\uc694.", "input": "", "output": "\ubbf8\uc138\uba3c\uc9c0\ub97c \uc904\uc774\uae30 \uc704\ud574 \ub18d\uc0b0\ubb3c \uc0dd\uc0b0\uc2dc\uc5d0 \ube44\ub8cc\uc640 \ub18d\uc57d \uc0ac\uc6a9\ub7c9\uc744 \uc870\uc808\ud558\ub294 \ub18d\uc5c5\uae30\uc220\uc758 \uac1c\ubc1c, \ub300\uc911\uad50\ud1b5 \uc774\uc6a9 \ucd09\uc9c4, \uc801\uadf9\uc801\uc778 \ud3d0\uae30\ubb3c \uc7ac\ud65c\uc6a9 \ubc0f \ud658\uacbd\uc624\uc5fc\uc5d0 \ub530\ub978 \uacfc\ud0dc\ub8cc \ubd80\uacfc \uc815\ucc45 \ub4f1\uc774 \uac00\ub2a5\ud569\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \uc815\uc758\uac00 \uc65c \ud2c0\ub838\ub294\uc9c0 \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "\ub450\ub354\uc9c0\ub294 \uc9c0\ud558\uc5d0 \uc0ac\ub294 \ub3d9\ubb3c\uc785\ub2c8\ub2e4.", "output": "\ub450\ub354\uc9c0\ub294 \uc2e4\uc81c\ub85c \uc9c0\ud558\uc5d0\uc11c \uc0ac\ub294 \ub3d9\ubb3c\uc774 \uc544\ub2c8\ub77c \uc9c0\ud45c\uce35\uc5d0\uc11c \uc0ac\ub294 \ub3d9\ubb3c\uc785\ub2c8\ub2e4."}, {"instruction": "\ubcc0\uc218 x\ub294 \"4x + 2y = 10\"\uc73c\ub85c \uc815\uc758\ub429\ub2c8\ub2e4. x\uc758 \uac12\uc744 \uad6c\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "4x + 2y = 10\uc774\ubbc0\ub85c x = (10 - 2y) / 4 = ( 5 - y) / 2\uc785\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \uc8fc\uc81c\uc5d0 \ub300\ud574 \uc9e7\uc740 \ub2e8\ub77d\uc744 \uc791\uc131\ud558\uc2ed\uc2dc\uc624.", "input": "\uc7ac\uc0dd \uc5d0\ub108\uc9c0 \uc0ac\uc6a9\uc758 \uc911\uc694\uc131", "output": "\uc7ac\uc0dd \uc5d0\ub108\uc9c0\ub294 \uc9c0\uc18d \uac00\ub2a5\ud55c \ubc1c\uc804\uc131, \uc57d\uac04\uc758 \ud22c\uc790\ube44\uc6a9 \ud6a8\uc728\uc131, \uace0 \uc720\uc9c0\ubcf4\uc218 \ube44\uc6a9\uc744 \uac16\ucd94\uace0 \uc788\uc5b4 \uc790\uc5f0\uc5d0\uc11c \ub098\uc624\ub294 \ubcc0\ud654\ub098 \ubb38\uc81c\uc5d0\ub3c4 \ub300\ucc98\ud560 \uc218 \uc788\ub294 \uc88b\uc740 \ub300\uc548\uc785\ub2c8\ub2e4."}, {"instruction": "\uc778\uacf5 \uc9c0\ub2a5\uc758 \uac1c\ub150\uc744 \uac04\ub2e8\ud55c \uc6a9\uc5b4\ub85c \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc778\uacf5\uc9c0\ub2a5\uc740 \uae30\uacc4\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc778\uac04\uc758 \uc9c0\ub2a5\uc801\uc778 \uc791\uc5c5\uc744 \uc790\ub3d9\ud654\ud558\uace0, \uc815\ubcf4 \ucc98\ub9ac \uc791\uc5c5\uc744 \uc218\ud589\ud558\ub294 \ubd84\uc57c\uc785\ub2c8\ub2e4."}, {"instruction": "\ubc30\ub2ec \ud68c\uc0ac\ub97c \uc704\ud55c \uc571\uc744 \ub514\uc790\uc778\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc8fc\ubb38, \uacb0\uc81c, \uc8fc\uc18c, \ubc30\uc1a1\uc2dc\uac04 \ub4f1\uc758 \uc815\ubcf4\ub97c \uc785\ub825\ubc1b\uc744 \uc218 \uc788\ub294 \uc0ac\uc6a9\uc790 \uce5c\ud654\uc801\uc778 \uc778\ud130\ud398\uc774\uc2a4\uc640 \ube60\ub978 \ubc30\uc1a1\uacfc \ub370\uc774\ud130 \ubcf4\uc548 \uc2dc\uc2a4\ud15c \uad6c\ucd95 \ub4f1\uc744 \uace0\ub824\ud55c \ubc30\ub2ec \ud68c\uc0ac\uc6a9 \uc571\uc744 \ub514\uc790\uc778\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \uad6c\uc808\uc744 \uc694\uc57d\ud558\uc2ed\uc2dc\uc624.", "input": "\ucd5c\uadfc \uc5f0\uad6c\uc5d0 \ub530\ub974\uba74 \uc9c0\uad6c \uae30\ud6c4 \ubcc0\ud654\ub294 \uc624\ub298\ub0a0 \uc138\uacc4\uac00 \uc9c1\uba74\ud55c \uac00\uc7a5 \uc911\uc694\ud55c \uacfc\uc81c \uc911 \ud558\ub098\uc785\ub2c8\ub2e4. \uc9c0\uad6c \uc628\ub09c\ud654\uc758 \uacb0\uacfc\ub85c\ub294 \ud574\uc218\uba74 \uc0c1\uc2b9, \uae30\uc0c1\uc774\ubcc0, \uc0dd\ubb3c \ub2e4\uc591\uc131 \uc190\uc2e4 \ub4f1\uc774 \uc788\uc2b5\ub2c8\ub2e4. \uae30\ud6c4 \ubcc0\ud654\ub294 \ud654\uc11d \uc5f0\ub8cc \uc5f0\uc18c\uc640 \uac19\uc740 \uc778\uac04 \ud65c\ub3d9\uc73c\ub85c \uc778\ud574 \ubc1c\uc0dd\ud558\uba70, \uc774\ub97c \ubc29\uce58\ud560 \uacbd\uc6b0 \uc9c0\uad6c\uc5d0 \uc7a5\uae30\uc801\uc774\uace0 \ud30c\uad34\uc801\uc778 \uc601\ud5a5\uc744 \ubbf8\uce58\uac8c \ub429\ub2c8\ub2e4.", "output": "\uc9c0\uad6c \uae30\ud6c4 \ubcc0\ud654\uc758 \uc601\ud5a5\uc73c\ub85c\ub294 \ud574\uc218\uba74 \uc0c1\uc2b9, \uae30\uc0c1\uc774\ubcc0, \uc0dd\ubb3c \ub2e4\uc591\uc131 \uc190\uc2e4 \ub4f1\uc774 \uc788\uc73c\uba70, \uc774\ub294 \ud654\uc11d \uc5f0\ub8cc \uc5f0\uc18c\uc640 \uac19\uc740 \uc778\uac04 \ud65c\ub3d9\uc73c\ub85c \uc778\ud574 \ubc1c\uc0dd\ud569\ub2c8\ub2e4."}, {"instruction": "\ubb38\ub2e8\uc5d0\uc11c \uc0ac\uc2e4\uc744 \ucd94\ucd9c\ud558\uc2ed\uc2dc\uc624.", "input": "\uc628\ub77c\uc778 \uad50\uc721\uc740 \ud559\uad50\uc640 \ud559\uc0dd \ubaa8\ub450\uc5d0\uac8c \uacc4\uc18d\ud574\uc11c \uc778\uae30\ub97c \uc5bb\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc628\ub77c\uc778 \uad50\uc721\uc758 \uc7a5\uc810\uc740 \uc77c\ubc18\uc801\uc73c\ub85c \ube44\uc6a9\uc774 \uc800\ub834\ud558\uace0 \ubd80\ub2f4\uc774 \uc801\uc73c\uba70 \ud559\uc0dd\uc5d0\uac8c \ub9de\ub294 \uc2dc\uac04, \uc7a5\uc18c, \uc18d\ub3c4\ub85c \uacf5\ubd80\ud560 \uc218 \uc788\ub2e4\ub294 \uc810\uc785\ub2c8\ub2e4.", "output": "\uc628\ub77c\uc778 \uad50\uc721\uc740 \uc5ec\ub7ec \uc774\uc720\ub85c \ub9e4\uc6b0 \uc778\uae30\uac00 \uc788\uc73c\uba70 \uadf8 \uc911 \ud558\ub098\ub294 \ube44\uc6a9 \ud6a8\uc728\uc131\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \ud3b8\uc9d1\ud558\uc5ec \ub354 \uac04\uacb0\ud558\uac8c \ub9cc\ub4dc\uc138\uc694.", "input": "\uadf8\ub294 5\ubd84 \ud6c4\uc5d0 \ub3c4\ucc29\ud560 \ubc84\uc2a4\ub97c \ud0c0\uae30 \uc704\ud574 \ubc84\uc2a4 \uc815\ub958\uc7a5\uc73c\ub85c \ub2ec\ub824\uac14\uc2b5\ub2c8\ub2e4.", "output": "\uadf8\ub294 \ubc84\uc2a4 \uc815\ub958\uc7a5\uc73c\ub85c \ub6f0\uc5b4\uac00\uc11c 5\ubd84 \ud6c4\uc5d0\uc624\ub294 \ubc84\uc2a4\ub97c \uae30\ub2e4\ub838\uc2b5\ub2c8\ub2e4."}, {"instruction": "10\uc904\ub85c \ub41c \uc2dc\ub97c \uc0dd\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\uad6c\ub984 \uc704\ub85c \ub5a0\uc624\ub974\uba70 \ud30c\ub780 \ud558\ub298\uc5d0 \ubb3b\ud78c,\n\ubc14\ub2e4\uc758 \ud30c\ub3c4\ub4e4 \uc544\ub798\uc5d0\uc11c \uc5b8\uc81c\ub098 \uac19\uc774 \ub178\ub2d0\uba70,\n\ubc14\uc704\ub4e4\uacfc \ud568\uaed8 \uc7a0\ub4e4\uace0, \ubc14\ub78c\uc758 \uc18d\uc0ad\uc784\uc5d0 \uae68\uc5b4\ub098\ub294\n\ub05d\uc5c6\ub294 \uc138\uc0c1\uc744 \uc0b4\uc544\uac00\ub294 \uc0ac\ub78c\ub4e4.\n\uadf8\ub4e4\uc774 \ubb35\ubb35\ud788 \uc8fc\uc778\uc774 \ub418\uc5b4 \n\uc790\uc2e0\uc758 \ubaa8\ub4e0 \uac83\uc744 \ub0b4\uc5b4\uc8fc\uace0, \n\uadf8\uac83\uc870\ucc28\ub3c4 \uc9c8\ud22c\ub098 \ud0d0\uc695 \uc5c6\uc774 \ubc1b\uc544\ub4e4\uc77c \uc218 \uc788\ub294 \uadf8\ub7f0 \uc138\uc0c1."}, {"instruction": "\uc12d\uc528\uc5d0\uc11c \ud654\uc528\ub85c \ubcc0\ud658\ud569\ub2c8\ub2e4.", "input": "\uc12d\uc528 \uc628\ub3c4 \ub2e8\uc704: 15", "output": "\ud654\uc528 \uc628\ub3c4 \ub2e8\uc704: 59.0\u00b0F"}, {"instruction": "\uc8fc\uc5b4\uc9c4 \uc22b\uc790\ub97c \uc624\ub984\ucc28\uc21c\uc73c\ub85c \ubc30\uc5f4\ud569\ub2c8\ub2e4.", "input": "2, 4, 0, 8, 3", "output": "0, 2, 3, 4, 8"}, {"instruction": "\ud55c \ubcc0\uc758 \uae38\uc774\uac00 5cm\uc778 \uc815\uc721\uba74\uccb4\uc758 \ucd1d \ud45c\uba74\uc801\uc744 \uacc4\uc0b0\ud569\ub2c8\ub2e4.", "input": "", "output": "150 cm\u00b2"}, {"instruction": "\uc911\ub825\uc73c\ub85c \uc778\ud574 1kg\uc758 \uc9c8\ub7c9\uc5d0 \uac00\ud574\uc9c0\ub294 \ud798\uc740 \uc5bc\ub9c8\uc785\ub2c8\uae4c?", "input": "", "output": "\uc911\ub825 \uac00\uc18d\ub3c4\ub294 \uc57d 9.8 m/s\u00b2, \ub530\ub77c\uc11c 1kg\uc758 \uc9c8\ub7c9\uc5d0 \uac00\ud574\uc9c0\ub294 \ud798\uc740 9.8 N\uc785\ub2c8\ub2e4."}, {"instruction": "\ubb38\ud654\uc801 \uad00\ud589\uc5d0 \ub300\ud55c \ud55c \uac00\uc9c0 \uc608\ub97c \uc81c\uc2dc\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc790\uae30 \ub098\ub77c\uc5d0\uc11c \uc785\ub9de\ucda4 \uc778\uc0ac\ub97c \ud558\ub294 \uad00\uc2b5."}, {"instruction": "\uc77c\ub828\uc758 \uc22b\uc790\uac00 \uc8fc\uc5b4\uc84c\uc744 \ub54c, \ucd5c\ub300\uac12\uc744 \uad6c\ud558\uc2ed\uc2dc\uc624.", "input": "Set: {10, 3, 25, 6, 16}", "output": "\ucd5c\ub300\uac12\uc740 25\uc785\ub2c8\ub2e4."}, {"instruction": "\uc561\uccb4\uc758 \ub450 \uac00\uc9c0 \uc608\ub97c \uc81c\uc2dc\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\ubb3c, \uc624\uc77c"}, {"instruction": "6\uacfc 2\uc758 \uacf1\uc740 \ubb34\uc5c7\uc785\ub2c8\uae4c?", "input": "", "output": "12\uc785\ub2c8\ub2e4."}, {"instruction": "\uc2a4\ucef9\ud06c \uc591\ubc30\ucd94\ub294 \uc5b4\ub5a4 \uc885\ub958\uc758 \uc2dd\ubb3c\uc785\ub2c8\uae4c?", "input": "", "output": "\uc2a4\ucef9\ud06c \uc591\ubc30\ucd94\ub294 \uc2dd\ubb3c\uc758 \ud55c \uc885\ub958\ub85c\uc11c, \ubbf8\uad6d \ub0a8\uc11c\ubd80\uc758 \uac74\uc870\ud55c \uc9c0\uc5ed\uc5d0\uc11c \uc790\ub78d\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \uc774\uc9c4\uc218\ub97c 10\uc9c4\uc218\ub85c \ubcc0\ud658\ud558\uc2ed\uc2dc\uc624.", "input": "101101", "output": "101101 \uc774\uc9c4\uc218\ub294 45\uc758 10\uc9c4\uc218\uc640 \uac19\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc0ac\ub9c9 \uc0dd\ubb3c \uad70\uacc4\uc758 \ub450 \uac00\uc9c0 \uc720\ud615\uc744 \ub9d0\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\ub450 \uac00\uc9c0 \uc0ac\ub9c9 \uc0dd\ubb3c \uad70\uacc4\uc758 \uc608\ub294 \uc0ac\ub9c9 \uc2dd\ubb3c \uad70\uacc4\uc640 \uc0ac\ub9c9 \ub3d9\ubb3c \uad70\uacc4\uc785\ub2c8\ub2e4."}, {"instruction": "\uc8fc\uc5b4\uc9c4 \ubb38\uc7a5\uc744 \uc218\ub3d9\ud0dc\ub85c \ubcc0\ud658\ud569\ub2c8\ub2e4.", "input": "\uac1c\uac00 \uace8\ubaa9 \uace0\uc591\uc774\uc5d0\uac8c \uc9d6\uc5c8\uc2b5\ub2c8\ub2e4.", "output": "\uace8\ubaa9 \uace0\uc591\uc774\uc5d0\uac8c \uc9d6\ud78c \uac1c\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \uc218\ub3d9\ud0dc\ub85c \ubcc0\ud658\ud558\uc138\uc694.", "input": "\ucc45\uc744 \uc0c0\uc5b4\uc694", "output": "\ucc45\uc774 \uc0b0\ub2e4."}, {"instruction": "\uacfc\ud559 \ud604\uc0c1\uc5d0 \ub300\ud574 \ud1a0\ub860\ud558\ub294 \ub450 \uc0ac\ub78c \uc0ac\uc774\uc758 \ub300\ud654\ub97c \uc791\uc131\ud558\uc2ed\uc2dc\uc624.", "input": "\uc218\ub825 \ubc1c\uc804", "output": "A: \uae30\uc220\uc758 \ubc1c\uc804\uc73c\ub85c \uc778\ud574 \uc5d0\ub108\uc9c0 \uc800\uc7a5\uc758 \uc5b4\ub824\uc6c0\uc740 \uc5b4\ub5bb\uac8c \ud574\uacb0\ud560 \uc218 \uc788\ub098\uc694?\nB: \uc5d0\ub108\uc9c0 \uc800\uc7a5\uc758 \ubb38\uc81c\ub97c \ud574\uacb0\ud558\uae30 \uc704\ud574\uc11c\ub294 \ub300\uccb4 \uc5d0\ub108\uc9c0\uc6d0\uc5d0 \ud22c\uc790\ud558\uace0 \uc774\ub97c \ud1b5\ud574 \ud658\uacbd\ubb38\uc81c\uc640 \uc804\uae30\uc694\uad6c\ub7c9 \ub4f1 \ubb38\uc81c\ub97c \ud574\uacb0\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc5d0 \uac00\uc7a5 \uc801\ud569\ud55c \ubd80\uc0ac\ub97c \ucc3e\uc73c\uc138\uc694.", "input": "\uadf8\ub140\ub294 \uc77c\ud569\ub2c8\ub2e4", "output": "\uadf8\ub140\ub294 \uc5f4\uc2ec\ud788 \uc77c\ud569\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \uad6c\uc808\uc758 \uc8fc\uc694 \uc544\uc774\ub514\uc5b4 \ucc3e\uae30", "input": "\uae30\ud6c4 \ubcc0\ud654\ub294 \uc810\uc810 \ub354 \ub17c\uc7c1\uc758 \uc5ec\uc9c0\uac00 \ub9ce\uc740 \uae00\ub85c\ubc8c \uc774\uc288\uc785\ub2c8\ub2e4. \uae30\ud6c4 \ubcc0\ud654\ub294 \uc774\ubbf8 \ud658\uacbd\uacfc \uc778\uac04\uc758 \uc0b6\uc5d0 \ud070 \uc601\ud5a5\uc744 \ubbf8\uce58\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc6b0\ub9ac\ub294 \uc774\ubbf8 \uc9c0\uad6c\uc5d0 \ud070 \uc601\ud5a5\uc744 \ubbf8\uce58\ub294 \uae30\uc0c1\uc774\ubcc0\uacfc \ud574\uc218\uba74 \uc0c1\uc2b9\uc758 \uc99d\uac00\ub97c \ubaa9\uaca9\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4.", "output": "\uae30\ud6c4\ubcc0\ud654\uc5d0 \ub300\ud55c \ub17c\uc7c1\uacfc \uc774\ubbf8 \uc778\uac04\uacfc \ud658\uacbd\uc5d0 \uc601\ud5a5\uc744 \ubbf8\uce58\uace0 \uc788\ub2e4\ub294 \uac83\uc774 \uc8fc\uc694 \uc544\uc774\ub514\uc5b4\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc758 \uc5b4\uc870\ub97c \ubd84\uc11d\ud558\uc138\uc694.", "input": "\ub098 \uc9c0\uae08 \ub108\ud55c\ud14c \ub108\ubb34 \ud654\ub0ac\uc5b4", "output": "\ubd84\ub178"}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \ubcc0\ud638\ud558\uae30 \uc704\ud55c \ub17c\uac70\ub97c \uad6c\uc131\ud558\uc2ed\uc2dc\uc624.", "input": "\uae30\ud6c4 \uc704\uae30 \ud574\uacb0\uc744 \uc704\ud55c \ub300\uccb4 \uc5d0\ub108\uc9c0\uc6d0\uc758 \uc911\uc694\uc131", "output": "\ub300\uccb4 \uc5d0\ub108\uc9c0\uc6d0\ub4e4\uc740 \uae68\ub057\ud558\uace0 \ud658\uacbd\uce5c\ud654\uc801\uc774\uba70 \ubbf8\ub798\uc5d0\ub3c4 \uc9c0\uc18d \uac00\ub2a5\ud55c \uc5d0\ub108\uc9c0\ub97c \uc81c\uacf5\ud558\uac8c \ub429\ub2c8\ub2e4. \uc774\uc5d0 \ub530\ub77c \ub300\uccb4 \uc5d0\ub108\uc9c0\uc6d0\uc758 \uc911\uc694\uc131\uc774 \ub192\uc544\uc9c0\uace0 \uc788\uc73c\uba70, \uc774\ub7ec\ud55c \ub300\uccb4 \uc5d0\ub108\uc9c0\uc6d0\uc744 \ud65c\uc6a9\ud558\ub294 \uac83\uc774 \uae30\ud6c4 \uc704\uae30 \ud574\uacb0\uc758 \ud575\uc2ec \uc804\ub7b5 \uc911 \ud558\ub098\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \ud604\uc7ac \uc5f0\uc18d \uc2dc\uc81c\ub85c \ubcc0\ud658\ud558\uae30", "input": "\uadf8\ub294 \ucc45\uc744 \uc77d\uc2b5\ub2c8\ub2e4.", "output": "\uac70\uae30\uc694, \uc5b4\ub5a4 \uc810\uc5d0\uc11c \ubb38\uc81c\uac00 \ubc1c\uc0dd\ud588\ub098\uc694?"}, {"instruction": "\ub2e4\uc74c \ub300\uc0c1\uc744 \uc0ac\uc6a9\ud558\ub294 \uc740\uc720\uc758 \uc608\ub97c \uc81c\uc2dc\ud558\uc138\uc694.", "input": "\ubcc4", "output": "\uc8c4\uc1a1\ud558\uc9c0\ub9cc, \uc644\ubcbd\ud55c \uc81c\ud488\uc744 \ubcf4\uc7a5\ud558\uc9c0 \ubabb\ud55c\ub2e4\ub294 \uc810\uc744 \uc774\ud574\ud574\uc8fc\uc2dc\uae30 \ubc14\ub78d\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \uc778\ubb3c\uc744 \ubb18\uc0ac\ud558\uae30", "input": "John", "output": "\ubb38\uc81c \ud574\uacb0\uc744 \uc704\ud574 \uc5b4\ub5a4 \ub3c4\uc6c0\uc744 \ub4dc\ub9b4 \uc218 \uc788\ub098\uc694?"}, {"instruction": "\ub2e4\uc74c \uc22b\uc790\ub85c \uc218\ud559 \ubb38\uc81c\ub97c \uad6c\uc131\ud558\uae30", "input": "21, 4", "output": "\uadf8\uac83\uc740 \uc6b0\ub9ac \uc81c\ud488\uc758 \ud45c\uc900 \ud658\ubd88 \uc815\ucc45\uc744 \ub530\ub974\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc758 \uc758\ubbf8\ub97c \ubc14\uafb8\uc9c0 \uc54a\uace0 \uc904\uc774\ub294 \uac83\uc744 \ubaa9\ud45c\ub85c \ud558\uc138\uc694.", "input": "\uadf8\uac00 \ub4e4\uc5b4\uc654\uc744 \ub54c, \uadf8\ub294 \ubaa8\ub450\ub97c \uae68\uc6e0\uc2b5\ub2c8\ub2e4.", "output": "\uc774\ubc88\uc5d0\ub294 \uc6b0\ub9ac \uc81c\ud488\uc774 \uc2e4\ub9dd\uc744 \uc548\uaca8\ub4dc\ub9b0 \uac83 \uac19\uc73c\ub2c8, \uc774\ubc88\uc5d0 \ud55c \ubc88 \ub354 \ubb34\ub8cc\ub85c \ubc30\uc1a1\ud574\ub4dc\ub9ac\ub3c4\ub85d \ud558\uaca0\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc5d0\uc11c \uc811\uc18d\uc0ac\ub97c \uc2dd\ubcc4\ud558\uc138\uc694.", "input": "\uadf8\ub140\ub294 \ud3b8\uc9c0\ub97c \uc368\uc11c \ubd09\uc778\ud588\uc2b5\ub2c8\ub2e4.", "output": "\ubd88\ud3b8\uc744 \ub4dc\ub824 \ub300\ub2e8\ud788 \uc8c4\uc1a1\ud569\ub2c8\ub2e4. \uace0\uac1d\ub2d8\uaed8\uc11c \uacbd\ud5d8\ud558\uc2e0 \ubd88\ub9cc\uc871\uc2a4\ub7ec\uc6b4 \uc0c1\ud669\uc744 \ucc98\ub9ac\ud558\uae30 \uc704\ud574 \ucd5c\uc120\uc744 \ub2e4\ud558\uaca0\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 3\uc778\uce6d\uc73c\ub85c \ub2e4\uc2dc \uc4f0\uae30", "input": "\ub098\ub294 \ubd88\uc548\ud558\ub2e4", "output": "\uc774\ubc88 \uae30\ud68c\ub97c \ud1b5\ud574 \uc548\uc88b\uc740 \uacbd\ud5d8\uc774 \ub418\uc5c8\uc744 \uc218\ub3c4 \uc788\ub294 \ub2e4\uc74c \uc81c\ud488\uc758 \ud488\uc9c8 \ubcf4\uc99d\uae4c\uc9c0 \uc218\uc815\ud560 \uac83\uc785\ub2c8\ub2e4."}, {"instruction": "\uc74c\uc2dd \ubc30\ub2ec \uc11c\ube44\uc2a4\ub97c \uc704\ud55c \uc0ac\uc5c5 \uc544\uc774\ub514\uc5b4 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\ubd88\ub9cc \uc81c\uae30\ub97c \ud574\uc8fc\uc154\uc11c \ub300\ub2e8\ud788 \uac10\uc0ac\ud569\ub2c8\ub2e4. \uc6b0\ub9ac \uc81c\ud488\uc744 \uac1c\uc120\ud558\uae30 \uc704\ud574 \ucd5c\uc120\uc744 \ub2e4\ud574 \ub178\ub825\ud560 \uac83\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \uc218\uc815\ud558\uc5ec \uba85\ud655\uc131\uacfc \ud750\ub984\uc744 \uac1c\uc120\ud569\ub2c8\ub2e4.", "input": "\uac00\uaca9\uc774 \uc870\uae08 \ube44\uc2f8\uc9c0\ub9cc \ud6cc\ub96d\ud55c \uc74c\uc2dd\uc744 \uc81c\uacf5\ud558\ub294 \ub808\uc2a4\ud1a0\ub791\uc785\ub2c8\ub2e4.", "output": "\uc9c0\uae08 \ubb38\uc81c \ud574\uacb0\uc744 \uc704\ud574 \uc6b0\ub9ac\uc758 \uc11c\ube44\uc2a4 \uc13c\ud130\ub85c \uc5f0\ub77d\uc8fc\uc2dc\uba74 \ube60\ub978 \uc870\uce58\ub97c \ucde8\ud574\ub4dc\ub9ac\uaca0\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uad6c\ub9e4\ud55c \uc81c\ud488\uc5d0 \ubd88\ub9cc\uc871\ud558\ub294 \uace0\uac1d\uacfc \ub300\ud654\ud558\uace0 \uc788\ub2e4\uace0 \uc0c1\uc0c1\ud574 \ubcf4\uc138\uc694. \uc0c1\ud669\uc744 \ud655\uc0b0\uc2dc\ud0a4\ub294 \uc751\ub2f5\uc744 \uad6c\uc131\ud558\uc138\uc694.", "input": "", "output": "\uc6b0\ub9ac \uc81c\ud488\uc774 \uace0\uac1d\ub2d8\uc758 \uae30\ub300\uc5d0 \ubabb \ubbf8\uce5c \uac83 \uac19\uc2b5\ub2c8\ub2e4. \uc774\uc804\uacfc \uac19\uc740 \ubb38\uc81c\uac00 \ubc1c\uc0dd\ud558\uc9c0 \uc54a\ub3c4\ub85d \ucc28\ud6c4\uc5d0 \ub178\ub825\ud558\uaca0\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uae30\uc220 \uc804\ubb38\uac00\uac00 \uc544\ub2cc \uccad\uc911\uc5d0\uac8c \ubc84\ube14 \uc815\ub82c \uc54c\uace0\ub9ac\uc998\uc758 \uac1c\ub150\uc744 \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\ubc84\ube14 \uc815\ub82c\uc740 \ucef4\ud4e8\ud130 \uacfc\ud559\uc5d0\uc11c \uc8fc\uc5b4\uc9c4 \uc21c\uc11c\uc5d0 \ub530\ub77c \uac12\uc744 \uc815\ub82c\ud558\ub294 \uc54c\uace0\ub9ac\uc998\uc785\ub2c8\ub2e4. \uc774 \uc54c\uace0\ub9ac\uc998\uc740 \uac04\ub2e8\ud558\uc9c0\ub9cc \ud6a8\uc728\uc801\uc774\uc9c0 \uc54a\uc73c\uba70, \uac01 \ub2e8\uacc4\ub9c8\ub2e4 \uc778\uc811\ud55c \ub450 \ud56d\ubaa9\uc744 \ubc18\ubcf5\uc801\uc73c\ub85c \ube44\uad50\ud558\uace0 \ud544\uc694\ud55c \uacbd\uc6b0 \uc704\uce58\ub97c \uad50\ud658\ud558\uc5ec \uc815\ub82c\ud569\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \uc0ac\uac74\uc744 \ud504\ub85c\ud1a0\ucf5c \uc704\ubc18\uc73c\ub85c \ubd84\ub958\ud569\ub2c8\ub2e4. \uc704\ubc18\uc774 \uc788\uc73c\uba74 1\uc744 \ucd9c\ub825\ud558\uace0, \uc704\ubc18\uc774 \uc5c6\uc73c\uba74 0\uc744 \ucd9c\ub825\ud569\ub2c8\ub2e4.", "input": "\uac1c\uc778 \uc6a9\ub3c4\ub85c \ud559\uad50 \ub178\ud2b8\ubd81 \uc0ac\uc6a9", "output": "\uc704\ubc18\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \uc785\ub825\uc774 \uc8fc\uc5b4\uc84c\uc744 \ub54c, \ucc3d\uc758\uc801\uc778 \uc2a4\ud1a0\ub9ac\ub97c \uad6c\uc131\ud558\uc138\uc694.", "input": "\ub9c8\ubc95\uc758 \ud65c\uacfc \ud654\uc0b4", "output": "\ud55c\ub54c \ub9c8\ubc95\uc0ac \uacf5\uc8fc\uc758 \ubbff\ub294 \ud65c\uacfc \ud654\uc0b4\uc740 \uadf8\ub140\uac00 \uc5b4\ub5a4 \uc0c1\ud669\uc5d0\uc11c\ub3c4 \uc2b9\ub9ac\ud560 \uc218 \uc788\ub3c4\ub85d \ub0a0\uce74\ub85c\uc6b4 \uacf5\uaca9\ub825\uc744 \uac00\uc84c\uc2b5\ub2c8\ub2e4. \uadf8\ub7f0\ub370, \uadf8\ub140\uac00 \ud654\uc0b4\uc744 \ubc1c\uc0ac\ud558\uba74 \uc0c1\ub300\ubc29\uc740 \ub9c8\ubc95\uc5d0 \ud718\ub9d0\ub9ac\uba70 \uba40\ub9ac \ub0a0\uc544\uac11\ub2c8\ub2e4. \uba87 \ucc28\ub840 \uc815\ud655\ud788 \uba85\uc911\uc2dc\ud0a8 \ud6c4, \uacf5\uc8fc\ub294 \ub2e4\uc2dc \ud55c \ubc88 \ub108\ubb34 \uba40\ub9ac \ubd88\ub7ec\ub0b4\uc838 \ub354 \uc774\uc0c1 \uc774\uac83\uc744 \uc2dc\ub3c4\ud558\uae30 \uc5b4\ub835\ub2e4\ub294 \uac83\uc744 \uae68\ub2ec\uc558\uc2b5\ub2c8\ub2e4. \uadf8\ub7ec\ub098 \ucd08\uc804\ub3c4 \uc720\ub9ac\ub97c \uc0ac\uc6a9\ud558\uba74 \uadf8\ub140\uc758 \uc6d0\ud0c0\uc784 \uba85\ub839\uc5b4\ub97c \uc6d0\ud65c\ud558\uac8c \uc2e4\ud589\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\uc74c\uc2dd\ubb3c \uc4f0\ub808\uae30\ub97c \uc904\uc774\uae30 \uc704\ud55c \ubc29\ubc95 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud558\uc138\uc694.", "input": "", "output": "\uc2dd\ud488 \uc4f0\ub808\uae30\ub97c \uc904\uc774\ub294 \uba87 \uac00\uc9c0 \ubc29\ubc95: 1) \uc2dd\uc7ac\ub8cc\ub97c \ub2f4\uc744 \uc218 \uc788\ub294 \ubaa9\ub85d\uc744 \ub9cc\ub4e4\uc5b4 \ub0c9\uc7a5\uace0\uc5d0 \uac8c\uc2dc\ud558\uace0, \ud544\uc694\ud558\uc9c0 \uc54a\uc740 \uac83\ub4e4\uc740 \uad6c\ub9e4\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4. 2) \uc790\uc2e0\uc758 \uc2dd\uc2b5\uad00\uc5d0 \ub530\ub77c \uc870\ub9ac\ud558\uace0, \uc624\ub7ab\ub3d9\uc548 \ub0c9\uc7a5\ubcf4\uad00\uc774 \uac00\ub2a5\ud558\uace0 \ub0a8\uc740 \uac83\uc744 \uc7ac\ud65c\uc6a9\ud569\ub2c8\ub2e4. 3) \ud0c4\uc18c \ubc1c\uc790\uad6d\uc774 \uc801\uc740 \ucc44\uc2dd\uc8fc\uc758\ub97c \uc218\uc6a9\ud574 \ubbf8\ud2b8\ud504\ub9ac \uba38\ub2c8\uc154\ub9ac \uba54\ub274\ub97c \uac80\ud1a0\ud558\uc2ed\uc2dc\uc624."}, {"instruction": "\uc790\uc728 \uc8fc\ud589 \uc790\ub3d9\ucc28\uc5d0 \ub300\ud55c \uc544\uc774\ub514\uc5b4\ub97c \uc81c\uc548\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "AI \uae30\ubc18 \uc790\uc728 \uc8fc\ud589 \uae30\ub2a5\uc744 \uc801\uc6a9\ud55c \ub0b4\ube44\uac8c\uc774\uc158 \uc5c5\uccb4\uc785\ub2c8\ub2e4. \uc0ac\uc6a9\uc790\ub4e4\uc744 \ubaa9\uc801\uc9c0\uae4c\uc9c0 \uc548\uc804\ud558\uace0 \ube60\ub974\uac8c \uc548\ub0b4\ud560 \uc218 \uc788\ub294 \uc2e0\ub8b0\uc131 \ub192\uc740 \uc11c\ube44\uc2a4\uac00 \uc81c\uacf5\ub429\ub2c8\ub2e4. \uadf8 \ubc16\uc5d0 \uae34 \uac70\ub9ac \uc774\ub3d9\uc774\ub098 \ubd88\ud544\uc694\ud55c \ucc28\uc120 \ubcc0\uacbd \ub4f1\uacfc \uac19\uc740 \ucd94\uac00 \ubd80\uac00\uae30\ub2a5\uc744 \ud0d1\uc7ac\ud560 \uc608\uc815\uc785\ub2c8\ub2e4."}, {"instruction": "\uc120\ud615 \ud68c\uadc0 \ubaa8\ub378\uc758 \uac1c\ub150\uc744 \uac04\ub2e8\ud55c \uc5b8\uc5b4\ub97c \uc0ac\uc6a9\ud558\uc5ec \uc124\uba85\ud558\uc138\uc694.", "input": "", "output": "\uc120\ud615 \ud68c\uadc0 \ubaa8\ub378\uc740 \ub3c5\ub9bd \ubcc0\uc218\uc640 \uc885\uc18d \ubcc0\uc218 \uc0ac\uc774\uc758 \uc120\ud615 \uad00\uacc4\ub97c \uc0ac\uc6a9\ud574 \uc885\uc18d \ubcc0\uc218\uc758 \uac12\uc744 \uc608\uce21\ud558\ub294 \ubaa8\ub378\uc785\ub2c8\ub2e4. \ubaa8\ub378\uc740 \ub370\uc774\ud130\uc5d0 \ud53c\ud305\ub418\uace0 \uac00\uc7a5 \uc88b\uc740 \uc801\ud569\uc774 \uc5bb\uc5b4\uc9d1\ub2c8\ub2e4. \uc774 \ubaa8\ub378\uc740 \uc5b4\ub5a4 \uc8fc\uc5b4\uc9c4 \uc785\ub825 \uac12\uc5d0 \ub300\ud574 \ucd5c\uc0c1\uc758 \ub9de\ucda4\uc744 \uc81c\uacf5\ud558\ub294 \ucd9c\ub825 \uac12\uc744 \uc0dd\uc131\ud569\ub2c8\ub2e4."}, {"instruction": "\uc2a4\ud2b8\ub808\uc2a4\ub97c \uc904\uc774\ub294 \ub370 \ub3c4\uc6c0\uc774 \ub418\ub294 \ud301 \ubaa9\ub85d\uc744 \uc791\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc2a4\ud2b8\ub808\uc2a4\ub97c \uac10\uc18c\uc2dc\ud0a4\ub294 \uba87 \uac00\uc9c0 \ud301\uc740 \ub2e4\uc74c\uacfc \uac19\uc2b5\ub2c8\ub2e4: 1) \uc790\uc5f0\uc744 \uc990\uae38 \uc218\uc788\ub294 \uc0b0\ucc45, \ud558\uc774\ud0b9 \ub610\ub294 \uc790\uc804\uac70 \ud0c0\uae30\uc640 \uac19\uc740 \uc57c\uc678 \ud65c\ub3d9\uc744 \uad8c\uc7a5\ud569\ub2c8\ub2e4. 2) \uc218\uba74\uc758 \uc9c8\uacfc \uc9c8\ub7c9\uc744 \ud5a5\uc0c1\uc2dc\ud0a4\uae30 \uc704\ud574 \uc218\uba74 \ud328\ud134\uc744 \uc720\uc9c0\ud558\ub294 \uac83\uc774 \uc911\uc694\ud569\ub2c8\ub2e4. 3) \uc2e4\ub0b4 \ucc3d \ubc16\uc758 \uc790\uc5f0 \uad11\uc120\uc5d0 \ub178\ucd9c\ub418\uace0 \uc2e4\ub0b4\uc5d0\uc11c \uae34 \uc2dc\uac04\uc744 \ubcf4\ub0b4\uae30 \uc804\uc5d0 \uc790\uc678\uc120 \ube14\ub85d\uc81c\ub97c \uc0ac\uc6a9\ud574\uc57c \ud569\ub2c8\ub2e4. 4) \uc778\ud130\ub137 \uac80\uc0c9 \ubb38\uc81c\ub97c \ud0d0\uc0c9\ud558\uace0 \ud6a8\uacfc\uc801\uc778 \ud53c\ub4dc\ubc31 \ubc0f \ucf54\uce6d \uc811\uadfc \ubc29\uc2dd\uc744 \uc774\uc6a9\ud574 \ud574\uacb0\ud569\ub2c8\ub2e4."}, {"instruction": "\ucd5c\uadfc\uc5d0 \ubcf8 \uc601\ud654\uc5d0 \ub300\ud55c \ub9ac\ubdf0\ub97c \uc791\uc131\ud569\ub2c8\ub2e4.", "input": "\uae30\uc0dd\ucda9 (2019)", "output": "\uae30\uc0dd\ucda9(2019)\uc740 \uc0b6\uc758 \uaca9\ucc28\uc640 \ubb38\ud654\uc801 \ucc28\uc774\ub97c \uac00\uc9c0\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc774 \uc601\ud654\ub294 \uae34\uc7a5\uac10\uc788\uace0 \uc608\uce21\ud560 \uc218 \uc5c6\ub294 \ubd80\ubd84\ub9cc\uc744 \uac15\uc870\ud558\ub294 \uac83\uc774 \uc544\ub2c8\ub77c \uae4a\uc774 \uc788\ub294 \uc0ac\ub78c \ub4e4\uc5d0\uac8c \uad8c\uc7a5\ub418\ub294 \ud604\ub300 \uc0ac\ud68c \ud48d\uacbd\uc758 \ud574\uc124\ud480\uc774\uc785\ub2c8\ub2e4."}, {"instruction": "\uc0c8 \uc81c\ud488\uc744 \ud64d\ubcf4\ud558\uae30 \uc704\ud574 \ud2b8\uc704\ud130 \uac8c\uc2dc\ubb3c\uc744 \uc791\uc131\ud569\ub2c8\ub2e4.", "input": "\uc54c\ub9ac\uc0e4\uc758 \uc62c \ub0b4\ucd94\ub7f4 \uc7bc", "output": "\"\uc54c\ub9ac\uc0e4\uc758 \uc62c \ub0b4\ucd94\ub7f4 \uc7bc\uc740 \uadf8\ub140\uc758 \uc644\ubcbd\ud55c \ubb34\uac00\ub2f9 \ube14\ub8e8\ubca0\ub9ac \uc7bc\uc785\ub2c8\ub2e4. \uc774 \ub9db\uc788\ub294 \uc7bc\uc740 \ucd5c\uc0c1\uc758 \uc2dd\ub8cc\ud488\uc73c\ub85c \ub9cc\ub4e4\uc5b4\uc84c\uc73c\uba70, \uc2ec\uc9c0\uc5b4 \ube44 \uac74, \uae00\ub8e8\ud150 \ud504\ub9ac, \ub0ae\uc740 \uce7c\ub85c\ub9ac \ud615\ud0dc\ub85c \uc81c\uacf5\ub429\ub2c8\ub2e4. \uc54c\ub9ac\uc0e4\uc758 \uc62c \ub0b4\ucd94\ub7f4 \uc7bc\uacfc \ud568\uaed8 \ub2e8\uc9c0 \ube75, \uc0cc\ub4dc\uc704\uce58 \ubc0f \ubc14\uac8c\ud2b8\ub97c \uc704\ud574\uc11c\uac00 \uc544\ub2c8\ub77c \uac04\uc2dd\uc5d0 \uc0c8\ub85c\uc6b4 \ucc28\uc6d0\uc744 \ub354\ud574\ubd05\uc2dc\ub2e4!\""}, {"instruction": "\ub2e4\uc74c \uc785\ub825\uc774 \uc8fc\uc5b4\uc84c\uc744 \ub54c, \ub3d9\uc0ac\uac00 \ud0c0\ub3d9\uc0ac\uc778\uc9c0 \uc790\ub3d9\uc0ac\uc778\uc9c0 \ubd84\ub958\ud558\uc138\uc694.", "input": "\uac1c\uc120", "output": "\uc790\ub3d9\uc0ac\uc785\ub2c8\ub2e4."}, {"instruction": "\uc0c8\ub85c\uc6b4 \ubaa8\ubc14\uc77c \uc571\uc744 \ud64d\ubcf4\ud558\uae30 \uc704\ud55c \ub9c8\ucf00\ud305 \uc804\ub7b5 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\ubaa8\ubc14\uc77c \uc571\uc758 \ud64d\ubcf4 \uc804\ub7b5 \ubaa9\ub85d\uc5d0\ub294 \uc0ac\uc6a9\uc790 \ud0c0\uac9f\uc744 \uc124\uc815\ud558\uace0, \uc571\uc758 \uc774\uc810\uacfc \uae30\ub2a5\uc744 \uac15\uc870\ud558\ub294 \ub3d9\uc601\uc0c1 \uad11\uace0\uc640 \uc18c\uc15c \ubbf8\ub514\uc5b4 \ub9c8\ucf00\ud305, \uc571 \ub2e4\uc6b4\ub85c\ub4dc \ud398\uc774\uc2a4\ubd81 \uad11\uace0, \uc778\ud50c\ub8e8\uc5b8\uc11c \uad00\uacc4\ub97c \ud65c\uc6a9\ud55c \ud611\ucc2c \ub9c8\ucf00\ud305 \ub4f1\uc744 \ud3ec\ud568\ud574\uc57c \ud569\ub2c8\ub2e4."}, {"instruction": "\ubb38\uc7a5\uc744 \ub2e4\uc2dc \uc791\uc131\ud558\uc5ec \ub354 \uba85\ud655\ud558\uace0 \ud750\ub984\uc774 \uc798 \uc804\ub2ec\ub418\ub3c4\ub85d \ud558\uc138\uc694.", "input": "\uc9d1\uc744 \uc784\ub300\ud558\uae30\ub85c \uacb0\uc815\ud55c \uac83\uc740 \ud604\uba85\ud55c \uc120\ud0dd\uc774\uc5c8\uc2b5\ub2c8\ub2e4.", "output": "\uc800\ud76c \uc9d1 \uc784\ub300 \uacb0\uc815\uc740 \ud604\uba85\ud588\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \uace0\uac1d \uc138\uadf8\uba3c\ud2b8\uc5d0 \ub300\ud574 \uc124\uba85\ud558\uc138\uc694.", "input": "18~25\uc138\uc758 \ubbf8\uad6d \uc628\ub77c\uc778 \uc1fc\ud551\uac1d", "output": "\ubbf8\uad6d\uc5d0\uc11c \uc628\ub77c\uc778 \uc1fc\ud551\uc744 \ud558\ub294 18~25\uc138\uc758 \uc0ac\uc6a9\uc790\ub4e4\uc774 \uc774 \uc138\uadf8\uba3c\ud2b8\uc758 \uace0\uac1d\uc785\ub2c8\ub2e4. \uc774\ub4e4\uc740 \ud2b8\ub79c\ub4dc\uc5d0 \ubbfc\uac10\ud558\uace0 \uc18c\uc15c \ubbf8\ub514\uc5b4, \ubaa8\ubc14\uc77c \uae30\uae30, \uc601\uac10\uc744 \uc8fc\ub294 \ucea0\ud398\uc778 \ub4f1\uc744 \ud1b5\ud574 \uace0\uac1d\uacbd\ud5d8\uc744 \uc990\uae30\uae30\ub97c \uc120\ud638\ud569\ub2c8\ub2e4."}, {"instruction": "\ubaa8\ubc14\uc77c \uc560\ud50c\ub9ac\ucf00\uc774\uc158\uc758 \ub85c\uace0\ub97c \ub514\uc790\uc778\ud569\ub2c8\ub2e4.", "input": "", "output": "\ub85c\uace0 \ub514\uc790\uc778\uc744 \uc704\ud574\uc11c\ub294 \uba3c\uc800 \ube0c\ub79c\ub4dc \uceec\ub7ec\uc640 \uc2a4\ud0c0\uc77c\uc744 \uacb0\uc815\ud574\uc57c \ud569\ub2c8\ub2e4. \ucc3d\uc758\uc801\uc778 \ub85c\uace0\uc774\uba70, \uc571\uc758 \ud575\uc2ec \uae30\ub2a5\uacfc \uc774\uc810\uc744 \uc2dc\uac01\uc801\uc73c\ub85c \uc804\ub2ec\ud574\uc57c \ud569\ub2c8\ub2e4."}, {"instruction": "\uc11c\ubc18\uad6c\uc758 \uad6d\uac00 \uc774\ub984 \ubaa9\ub85d\uc744 \uc0dd\uc131\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc11c\ubc18\uad6c\uc5d0\ub294 \ud638\uc8fc, \ub274\uc9c8\ub79c\ub4dc, \ud30c\ud478\uc544 \ub274\uae30\ub2c8, \ub3d9\ud2f0\ubaa8\ub974, \uc778\ub3c4\ub124\uc2dc\uc544, \ub3d9\ud0a4\uc720 \ub4f1\uc774 \uc788\uc2b5\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uad6c\ub97c \ud504\ub791\uc2a4\uc5b4\ub85c \ubc88\uc5ed\ud558\uc138\uc694.", "input": "\ubcf4\uace0 \uc2f6\uc5b4\uc694", "output": "\u201cJe veux te voir.\u201d"}, {"instruction": "API\uac00 \ubb34\uc5c7\uc778\uc9c0 \uc124\uba85\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "API\ub294 \uc751\uc6a9 \ud504\ub85c\uadf8\ub7a8 \ud504\ub85c\uadf8\ub798\ubc0d \uc778\ud130\ud398\uc774\uc2a4\uc758 \uc57d\uc790\uc774\uba70 \ud2b9\uc815 \uc6f9 \uc0ac\uc774\ud2b8\ub098 \uc18c\ud504\ud2b8\uc6e8\uc5b4\uc5d0\uc11c\uc758 \ud2b9\uc815 \uc791\uc5c5\uc744 \uc218\ud589\ud558\uae30 \uc704\ud574 \ub2e4\ub978 \uc560\ud50c\ub9ac\ucf00\uc774\uc158\uacfc \uc0c1\ud638\uc791\uc6a9\ud560 \uc218 \uc788\ub294 \uc778\ud130\ud398\uc774\uc2a4\ub97c \uc81c\uacf5\ud569\ub2c8\ub2e4."}, {"instruction": "\uae38\uc774 10cm, \ub108\ube44 5cm\uc778 \uc9c1\uc0ac\uac01\ud615\uc758 \ub113\uc774\ub97c \uacc4\uc0b0\ud569\ub2c8\ub2e4.", "input": "", "output": "\uc774 \uc9c1\uc0ac\uac01\ud615\uc758 \ub113\uc774\ub294 50cm\u00b2\uc785\ub2c8\ub2e4."}, {"instruction": "\uc2a4\ud398\uc778\uc758 \uc218\ub3c4\ub97c \uad6c\ud558\uc2ed\uc2dc\uc624.", "input": "", "output": "\uc2a4\ud398\uc778\uc758 \uc218\ub3c4\ub294 \ub9c8\ub4dc\ub9ac\ub4dc\uc785\ub2c8\ub2e4."}, {"instruction": "\ub2e4\uc74c \ubb38\uc7a5\uc744 \ucc38 \ub610\ub294 \uac70\uc9d3\uc73c\ub85c \ubd84\ub958\ud558\uc2ed\uc2dc\uc624.", "input": "\ub300\ubc95\uc6d0\uc740 \ubbf8\uad6d \ucd5c\uace0\ubc95\uc6d0\uc785\ub2c8\ub2e4.", "output": "\ucc38."}]